  select
    deezer_id,
    max(music) as music,
    max(artist) as artist,
    sum(play_rev_ranking) as ranking
  from (
    select
      deezer_id,
      id,
      music,
      artist,
      release_date,
      1 as play_rev_ranking
    from music
    where deezer_id is not null
      and isrc like 'BR%'
      and origin = 'Spotify'
  ) x
  group by deezer_id
  having ranking >= 1
  order by ranking
  limit 200

